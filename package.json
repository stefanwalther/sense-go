{
  "name": "sense-go",
  "version": "0.6.5",
  "description": "Validate, package and deploy your Qlik Sense Visualization Extension projects with ease.",
  "keywords": [
    "deployment",
    "extensions",
    "grunt",
    "gulp",
    "less",
    "mashups",
    "packaging",
    "qlik",
    "qlik-sense",
    "qlik-sense-desktop",
    "qrs",
    "ssh",
    "validation",
    "visualization-extensions",
    "wbfolder.wbl",
    "web-projects",
    "zip"
  ],
  "homepage": "https://github.com/stefanwalther/sense-go",
  "bugs": {
    "url": "https://github.com/stefanwalther/sense-go/issues"
  },
  "license": "MIT",
  "author": "Stefan Walther (https://github.com/stefanwalther)",
  "main": "./lib/index.js",
  "bin": {
    "sense-go": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stefanwalther/sense-go.git"
  },
  "scripts": {
    "test": "mocha test/*.spec.js",
    "verb": "verb && if [[ `git status -z --porcelain` ]]; then git commit -a -m 'docs: Update Readme'; else echo '\n\nNO CHANGES, NOTHING TO PUSH'; fi"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "async": "^2.0.0",
    "chalk": "^1.1.0",
    "deep-extend": "^0.4.1",
    "del": "^2.2.1",
    "expand-tilde": "^1.2.2",
    "extend-shallow": "^2.0.1",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.5",
    "graceful-fs": "^4.1.3",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.1",
    "gulp-bump": "^2.1.0",
    "gulp-concat": "^2.5.2",
    "gulp-debug": "^2.0.1",
    "gulp-filter": "^4.0.0",
    "gulp-git": "^1.8.0",
    "gulp-htmlmin": "^2.0.0",
    "gulp-if": "^2.0.1",
    "gulp-jsonlint": "^1.1.0",
    "gulp-jsonminify": "^1.0.0",
    "gulp-less": "^3.1.0",
    "gulp-load-plugins": "^1.2.4",
    "gulp-mocha": "^2.1.2",
    "gulp-nop": "0.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-replace-task": "^0.11.0",
    "gulp-simple-task-loader": "^1.3.8",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-ssh": "^0.6.0",
    "gulp-strip-debug": "^1.0.2",
    "gulp-tag-version": "^1.2.1",
    "gulp-touch": "^1.0.1",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.8",
    "gulp-zip": "^3.0.2",
    "js-yaml": "^3.4.6",
    "liftoff": "^2.2.4",
    "lodash": "^4.13.1",
    "minimatch": "^3.0.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "sanitize-filename": "^1.6.0",
    "sense-loc": "^0.4.0",
    "spawn-cmd": "0.0.2",
    "tildify": "^1.2.0",
    "timestamp": "0.0.1",
    "v8flags": "^2.0.11"
  },
  "devDependencies": {
    "assertion-error": "^1.0.2",
    "chai": "latest",
    "chai-files": "^1.3.0",
    "del": "^2.2.1",
    "ghooks": "^1.3.2",
    "mocha": "*",
    "should": "*"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run verb",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "data": {
      "twitter": "waltherstefan",
      "travis_url": "stefanwaltehr"
    }
  }
}
